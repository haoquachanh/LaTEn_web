{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "cross-env NEXT_PUBLIC_APP_ENV=staging next dev",
    "dev:prod": "cross-env NEXT_PUBLIC_APP_ENV=production next dev",
    "build": "next build",
    "build:staging": "cross-env NEXT_PUBLIC_APP_ENV=staging next build",
    "build:prod": "cross-env NEXT_PUBLIC_APP_ENV=production next build",
    "start": "next start",
    "start:staging": "cross-env NEXT_PUBLIC_APP_ENV=staging next start",
    "start:prod": "cross-env NEXT_PUBLIC_APP_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ci": "npm run lint && npm run type-check && npm run test && npm run build"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.0.10",
    "axios": "^1.6.8",
    "class-validator": "^0.14.0",
    "critters": "^0.0.23",
    "lodash": "^4.17.21",
    "next": "14.1.4",
    "next-intl": "^3.26.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "swiper": "^11.2.10",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "daisyui": "^4.10.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
